{
  "deployment": {
    "containerName": "fastify-tmpl",
    "imageName": "tuiteraz/fastify-tmpl"
  },
  "runtime": {
    "environment": "development",
    "logLevel": "debug",
    "port": 9000
  },
  "speechProviders": {
    "playht": {
      "apiKey": "playht-api-key",
      "userId": "playht-user-id"
    },
    "deepgram": {
      "apiKey": "deepgram-api-key"
    }
  },
  "composition": {
    "newLinePauseMilliSeconds": 1000
  }
}